{
  "name": "g3-tsx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:pycharm": "cd sites/pycharm-guide && node --require esbuild-register ../../node_modules/@11ty/eleventy/cmd.js --incremental --config=../../eleventy.config.js --serve --watch",
    "build:pycharm": "npm run clean:pycharm && cd sites/pycharm-guide && node --require esbuild-register ../../node_modules/@11ty/eleventy/cmd.js --config=../../eleventy.config.js --pathprefix=/pycharm/guide",
    "clean:pycharm": "rimraf .11ty-vite sites/pycharm-guide/_site sites/pycharm-guide/.11ty-vite",
    "dev:webstorm": "cd sites/webstorm-guide && node --require esbuild-register ../../node_modules/@11ty/eleventy/cmd.js --config=../../eleventy.config.js --serve --watch",
    "build:webstorm": "npm run clean:webstorm && cd sites/webstorm-guide && node --require esbuild-register ../../node_modules/@11ty/eleventy/cmd.js --config=../../eleventy.config.js --pathprefix=/webstorm/guide",
    "clean:webstorm": "rimraf sites/webstorm-guide/_site sites/webstorm-guide/.11ty-vite",
    "clean": "npm run clean:pycharm && npm run clean:webstorm",
    "test": "vitest run"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "@sinclair/typebox": "^0.25.24",
    "@testing-library/dom": "^8.20.0",
    "@types/luxon": "^3.2.0",
    "@types/markdown-it": "^12.2.3",
    "@types/vhtml": "^2.2.4",
    "@types/video.js": "^7.3.50",
    "@vitest/coverage-c8": "^0.28.4",
    "command-line-args": "^5.2.1",
    "copyfiles": "^2.4.1",
    "esbuild-register": "^3.3.2",
    "happy-dom": "^8.2.2",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5",
    "upath": "^2.0.1",
    "vhtml": "^2.2.0",
    "vite": "^4.2.0",
    "vitest": "^0.29.3",
    "vite-plugin-image-optimizer": "^1.1.0",
    "lunr" : "^2.3.9",
    "prism-themes": "^1.9.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "node-html-parser": "^6.1.5",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "sharp" : "^0.31.3",
    "svgo" : "^3.0.2",
    "plyr": "^3.7.7",
    "@11ty/eleventy-fetch": "^4.0.0"
  },
  "volta": {
    "node": "19.6.0"
  }
